{
  "name": "Lucid Chart Document Carousel Removal",
  "description": "Removes the carousel in lucidchart document section",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
   {
     "matches": ["https://lucid.app/*"],
     "js": ["flexbox.js"]
   }
 ],
  "icons": {
    "16": "/images/lucidchart_94941.png",
    "32": "/images/lucidchart_94941.png",
    "64": "/images/lucidchart_94941.png"
  }
}
